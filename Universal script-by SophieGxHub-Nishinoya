-- LocalScript
-- Coloque este LocalScript em StarterPlayer -> StarterPlayerScripts

local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Criar ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "WelcomeGui"
screenGui.Parent = playerGui

-- Frame da borda (maior, roxo)
local borderFrame = Instance.new("Frame")
borderFrame.Size = UDim2.new(0.42, 0, 0.12, 0) -- ligeiramente maior
borderFrame.Position = UDim2.new(0.29, 0, 0.44, 0)
borderFrame.BackgroundColor3 = Color3.fromRGB(138, 43, 226) -- roxo da borda
borderFrame.BorderSizePixel = 0
borderFrame.BackgroundTransparency = 1 -- inicia invis√≠vel
borderFrame.Parent = screenGui

local borderCorner = Instance.new("UICorner")
borderCorner.CornerRadius = UDim.new(0, 50)
borderCorner.Parent = borderFrame

-- Frame principal (c√°psula interna)
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0.4, 0, 0.1, 0)
frame.Position = UDim2.new(0.01, 0, 0.01, 0)
frame.BackgroundColor3 = Color3.fromRGB(75, 0, 130)
frame.BorderSizePixel = 0
frame.BackgroundTransparency = 1 -- inicia invis√≠vel
frame.Parent = borderFrame

local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 50)
corner.Parent = frame

-- Texto
local textLabel = Instance.new("TextLabel")
textLabel.Size = UDim2.new(1, 0, 1, 0)
textLabel.Position = UDim2.new(0, 0, 0, 0)
textLabel.BackgroundTransparency = 1
textLabel.Text = "Welcome to SophieHub-SFZ"
textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
textLabel.TextScaled = true
textLabel.Font = Enum.Font.GothamBold
textLabel.TextTransparency = 1 -- inicia invis√≠vel
textLabel.Parent = frame

-- Fun√ß√£o fade-in
local function fadeIn(duration)
    local step = 0.05
    local increment = step / duration
    local transparency = 1
    while transparency > 0 do
        transparency = math.max(transparency - increment, 0)
        frame.BackgroundTransparency = transparency
        borderFrame.BackgroundTransparency = transparency
        textLabel.TextTransparency = transparency
        wait(step)
    end
end

-- Fun√ß√£o fade-out
local function fadeOut(duration)
    local step = 0.05
    local increment = step / duration
    local transparency = 0
    while transparency < 1 do
        transparency = math.min(transparency + increment, 1)
        frame.BackgroundTransparency = transparency
        borderFrame.BackgroundTransparency = transparency
        textLabel.TextTransparency = transparency
        wait(step)
    end
end

-- Fun√ß√£o pulse para a borda
local function pulseBorder(duration, times)
    for i = 1, times do
        -- Aumenta a clareza
        for t = 0, 1, 0.05 do
            borderFrame.BackgroundColor3 = Color3.fromRGB(
                138 + math.floor(20 * t),
                43 + math.floor(20 * t),
                226 + math.floor(20 * t)
            )
            wait(duration * 0.05)
        end
        -- Volta ao normal
        for t = 0, 1, 0.05 do
            borderFrame.BackgroundColor3 = Color3.fromRGB(
                158 - math.floor(20 * t),
                63 - math.floor(20 * t),
                246 - math.floor(20 * t)
            )
            wait(duration * 0.05)
        end
    end
end

-- Executar anima√ß√£o
spawn(function() fadeIn(0.5) end)   -- fade-in
wait(0.5)                           -- espera o fade-in terminar

-- Pulse borda enquanto vis√≠vel
spawn(function() pulseBorder(0.5, 5) end)

wait(5)                             -- mant√©m vis√≠vel
fadeOut(0.5)                        -- fade-out
screenGui:Destroy()

wait(1) 
