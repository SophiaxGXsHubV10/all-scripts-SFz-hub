local pages = {}

local function createSubFrame(page)
	local sf = Instance.new("ScrollingFrame")
	sf.Parent = page
	sf.Position = UDim2.new(0, 10, 0, 50)
	sf.Size = UDim2.new(1, -20, 1, -60)
	sf.CanvasSize = UDim2.new(0, 0, 0, 0)
	sf.BackgroundTransparency = 1
	sf.BorderSizePixel = 0
	sf.ScrollBarThickness = 6

	local sl = Instance.new("UIListLayout", sf)
	sl.Padding = UDim.new(0, 8)
	sl.HorizontalAlignment = Enum.HorizontalAlignment.Center

	sl:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		sf.CanvasSize = UDim2.new(0, 0, 0, sl.AbsoluteContentSize.Y + 10)
	end)

	return function(text, callback)
		local b = Instance.new("TextButton", sf)
		b.Size = UDim2.new(1, -10, 0, 40)
		b.Text = text
		b.BackgroundColor3 = Color3.fromRGB(80, 40, 140)
		b.TextColor3 = Color3.new(1, 1, 1)
		b.Font = Enum.Font.GothamBold
		b.TextSize = 14
		b.BorderSizePixel = 0
		Instance.new("UICorner", b).CornerRadius = UDim.new(0, 8)
		b.MouseButton1Click:Connect(function() pcall(callback) end)
	end
end

