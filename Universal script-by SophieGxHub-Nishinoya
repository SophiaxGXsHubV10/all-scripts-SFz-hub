wait(1) --// Pequeno delay para garantir carregamento
local Players = game:GetService("Players")
local player = Players.LocalPlayer

--// GUI
local gui = Instance.new("ScreenGui")
gui.Name = "HubCompletoJogos"
gui.ResetOnSpawn = false
gui.Parent = player:WaitForChild("PlayerGui")

--// Frame principal
local main = Instance.new("Frame")
main.Parent = gui
main.Size = UDim2.new(0, 720, 0, 420)
main.Position = UDim2.new(0.5, -360, 0.5, -210)
main.BackgroundColor3 = Color3.fromRGB(35, 35, 40)
main.Active = true
main.Draggable = true
main.BorderSizePixel = 0

Instance.new("UICorner", main).CornerRadius = UDim.new(0, 15)
local stroke = Instance.new("UIStroke", main)
stroke.Color = Color3.fromRGB(90, 40, 160)
stroke.Thickness = 2

--// Sidebar (Menu Lateral)
local sidebar = Instance.new("ScrollingFrame")
sidebar.Parent = main
sidebar.Size = UDim2.new(0, 220, 1, -20)
sidebar.Position = UDim2.new(0, 10, 0, 10)
sidebar.CanvasSize = UDim2.new(0, 0, 0, 0)
sidebar.ScrollBarImageTransparency = 0.8
sidebar.ScrollBarThickness = 4
sidebar.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
sidebar.BorderSizePixel = 0

Instance.new("UICorner", sidebar).CornerRadius = UDim.new(0, 15)
local layout = Instance.new("UIListLayout", sidebar)
layout.Padding = UDim.new(0, 6)
layout.HorizontalAlignment = Enum.HorizontalAlignment.Center

layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
	sidebar.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 20)
end)

--// Conte√∫do (Lado Direito)
local content = Instance.new("Frame")
content.Parent = main
content.Position = UDim2.new(0, 240, 0, 10)
content.Size = UDim2.new(1, -250, 1, -20)
content.BackgroundColor3 = Color3.fromRGB(45, 45, 50)
content.BorderSizePixel = 0
Instance.new("UICorner", content).CornerRadius = UDim.new(0, 15)

--// LISTA DE CATEGORIAS
local tabs = {
	"üå≤99 noites",
	"üëΩ Steal a brainrot", -- NOME ALTERADO AQUI
	"üçé Blox Fruits",
	"üèê Voleyball Legends",
	"‚öΩ Blue Lock",
	"üé≠ Forsaken",
	"üå¥ Grow a Garden",
	"üêü Fish",
	"üëä Campos de Batalha",
	"üõ∂ Build a Boat",
	"üî™ MM2",
	"‚õèÔ∏è The Forge",
	"üó°Ô∏è Blade Ball",
	"‚ò†Ô∏è Death Ball",
	"üè† Brookheaven RP",
	"üöÇ Trilhos Mortos"
}
local pages = {}

--// Fun√ß√£o de Sub-Abas (Bot√µes de Script)
local function createSubFrame(page)
	local sf = Instance.new("ScrollingFrame")
	sf.Parent = page
	sf.Position = UDim2.new(0, 10, 0, 50)
	sf.Size = UDim2.new(1, -20, 1, -60)
	sf.CanvasSize = UDim2.new(0, 0, 0, 0)
	sf.ScrollBarImageTransparency = 0.5
	sf.BackgroundTransparency = 1
	sf.BorderSizePixel = 0
	sf.ScrollBarThickness = 6

	local sl = Instance.new("UIListLayout", sf)
	sl.Padding = UDim.new(0, 8)
	sl.HorizontalAlignment = Enum.HorizontalAlignment.Center

	sl:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		sf.CanvasSize = UDim2.new(0, 0, 0, sl.AbsoluteContentSize.Y + 10)
	end)

	return function(text, callback)
		local b = Instance.new("TextButton", sf)
		b.Size = UDim2.new(1, -10, 0, 40)
		b.Text = text
		b.BackgroundColor3 = Color3.fromRGB(80, 40, 140)
		b.TextColor3 = Color3.new(1, 1, 1)
		b.Font = Enum.Font.GothamBold
		b.TextSize = 14
		b.BorderSizePixel = 0
		Instance.new("UICorner", b).CornerRadius = UDim.new(0, 8)
		
		b.MouseButton1Click:Connect(function()
			pcall(callback)
		end)
	end
end

--// GERAR ABAS
for i, name in ipairs(tabs) do
	-- Bot√£o Lateral
	local button = Instance.new("TextButton", sidebar)
	button.Size = UDim2.new(0.9, 0, 0, 35)
	button.Text = name
	button.BackgroundColor3 = Color3.fromRGB(60, 30, 100)
	button.TextColor3 = Color3.new(1, 1, 1)
	button.Font = Enum.Font.Gotham
	button.TextSize = 13
	button.TextTruncate = Enum.TextTruncate.AtEnd
	Instance.new("UICorner", button).CornerRadius = UDim.new(0, 8)

	-- P√°gina de Conte√∫do
	local page = Instance.new("Frame", content)
	page.Size = UDim2.new(1, 0, 1, 0)
	page.Visible = (i == 1)
	page.BackgroundTransparency = 1
	pages[name] = page

	-- T√≠tulo da P√°gina
	local title = Instance.new("TextLabel", page)
	title.Size = UDim2.new(1, 0, 0, 40)
	title.Text = name
	title.Font = Enum.Font.GothamBlack
	title.TextSize = 24
	title.TextColor3 = Color3.fromRGB(180, 140, 255)
	title.BackgroundTransparency = 1

	local addSub = createSubFrame(page)

	--// L√ìGICA DOS JOGOS
	if name == "üå≤99 noites" then
		addSub("Voidware (sem key)", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/VapeVoidware/VW-Add/main/nightsintheforest.lua"))() end)
		addSub("H4x (com key)", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/H4xScripts/Loader/refs/heads/main/loader.lua"))() end)
		addSub("Fox name (sem key)", function() loadstring(game:HttpGet("https://rawscripts.net/raw/99-Nights-in-the-Forest-Foxname-Hub-57131"))() end)

	elseif name == "üëΩ Steal a brainrot" then -- VERIFICA√á√ÉO ATUALIZADA AQUI
		addSub("Lennon hub ‚≠ê", function() loadstring(game:HttpGet("https://pastefy.app/MJw2J4T6/raw"))() end)
		addSub("Lumora ‚≠ê", function() loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/3e7e5539bf07cd98f855ebb0c29c8081.lua"))() end)
		addSub("Overflow (com key)", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/OverflowBGSI/Overflow/refs/heads/main/loader.txt"))() end)
		addSub("Cartola hub (com key)", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/Davi999z/Cartola-Hub/refs/heads/main/StealABrainrot.lua", true))() end)

	elseif name == "üçé Blox Fruits" then
		addSub("Hoho Hub", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/acsu123/HOHO_H/main/Loading_UI"))() end)
		addSub("W-Azure (Auto Farm)", function() loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/3b2169cf53bc6104dabe8e19562e5cc2.lua"))() end)

	elseif name == "üèê Voleyball Legends" then
		addSub("Infinite Yield (Admin)", function() loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))() end)
		
	elseif name == "‚öΩ Blue Lock" then
		addSub("Infinite Yield (Admin)", function() loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))() end)

	elseif name == "üé≠ Forsaken" then
		addSub("Script Forsaken (Exemplo)", function() print("Coloque o script aqui") end)

	elseif name == "üå¥ Grow a Garden" then
		addSub("Infinite Yield (Admin)", function() loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))() end)

	elseif name == "üêü Fish" then
		addSub("Speed Hub X", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/AhmadV99/Speed-Hub-X/main/Speed%20Hub%20X.lua"))() end)

	elseif name == "üëä Campos de Batalha" then
		addSub("Kade Hub (Strongest)", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/skibiditoiletfan2007/KadeHub/main/KadeHub.lua"))() end)

	elseif name == "üõ∂ Build a Boat" then
		addSub("V0rq (Auto Build)", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/V0rq/V0rq/main/Bafb.lua"))() end)

	elseif name == "üî™ MM2" then
		addSub("Eclipse Hub", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/Ethanoj1/EclipseMM2/master/Script"))() end)

	elseif name == "‚õèÔ∏è The Forge" then
		addSub("Infinite Yield (Admin)", function() loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))() end)

	elseif name == "üó°Ô∏è Blade Ball" then
		addSub("Redz Hub", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/BladeBall/main/redz9999"))() end)

	elseif name == "‚ò†Ô∏è Death Ball" then
		addSub("Infinite Yield (Admin)", function() loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))() end)

	elseif name == "üè† Brookheaven RP" then
		addSub("Ice Hub", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/IceMael7/IceHub/main/Brookhaven"))() end)

	elseif name == "üöÇ Trilhos Mortos" then
		addSub("Infinite Yield (Admin)", function() loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))() end)
	end

	button.MouseButton1Click:Connect(function()
		for _, p in pairs(pages) do p.Visible = false end
		page.Visible = true
	end)
end

if pages[tabs[1]] then
	pages[tabs[1]].Visible = true
end

warn("‚úÖ HUB COMPLETO CARREGADO!")
